{
  "name": "catch-of-the-day",
  "version": "1.0.0",
  "description": "Catch of the Day is a website used in React for Beginners",
  "main": "",
  "scripts": {
    "start": "react-scripts start",
    "watch": "concurrently --names \"webpack, stylus\" --prefix name \"npm run start\" \"npm run styles:watch\"",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "styles": "stylus -u autoprefixer-stylus ./src/css/style.styl -o ./src/css/style.css",
    "styles:watch": "stylus -u autoprefixer-stylus -w ./src/css/style.styl -o ./src/css/style.css",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Asjas/Catch-of-the-Day.git"
  },
  "author": "A-J Roos",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Asjas/Catch-of-the-Day/issues"
  },
  "homepage": "https://github.com/Asjas/Catch-of-the-Day#readme",
  "dependencies": {
    "@firebase/app-types": "^0.1.0",
    "history": "4.7.2",
    "prettier": "^1.10.2",
    "re-base": "3.2.2",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-router-dom": "^4.2.2",
    "react-transition-group": "1.x"
  },
  "devDependencies": {
    "autoprefixer-stylus": "0.14.0",
    "babel-eslint": "8.2.1",
    "concurrently": "3.5.1",
    "eslint": "4.18.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "2.45.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "7.6.1",
    "react-scripts": "1.1.1",
    "stylus": "0.54.5"
  }
}
